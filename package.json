{
  "name": "jyotish-user-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prestart-disabled": "node fix-metro-import.js",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "fix-metro": "node fix-metro-import.js",
    "fix-build": "node fix-build-issues.js",
    "clean": "npx expo install --fix && cd android && ./gradlew clean && cd ..",
    "build-android": "npx expo run:android --variant release",
    "prebuild": "npx expo install --fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "axios": "^1.6.2",
    "expo": "~54.0.0",
    "expo-asset": "~12.1.0",
    "expo-av": "~16.1.0",
    "expo-constants": "~18.1.0",
    "expo-dev-client": "~5.2.0",
    "expo-device": "~8.1.0",
    "expo-font": "~14.1.0",
    "expo-linear-gradient": "~15.1.0",
    "expo-notifications": "~0.32.0",
    "expo-status-bar": "~2.3.0",
    "expo-system-ui": "~6.1.0",
    "expo-updates": "~0.29.0",
    "moment": "^2.30.1",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-audio-record": "^0.2.2",
    "react-native-edge-to-edge": "^1.0.4",
    "react-native-fbsdk-next": "^13.4.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-google-places-autocomplete": "^2.5.7",
    "react-native-paper": "^5.14.5",
    "react-native-razorpay": "^2.3.0",
    "react-native-reanimated": "~3.17.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-webview": "13.13.5",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@react-native-community/cli": "^20.0.1",
    "metro": "^0.82.0",
    "metro-resolver": "^0.82.0"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-razorpay",
          "react-native-webrtc",
          "react-native-audio-record",
          "socket.io-client"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
